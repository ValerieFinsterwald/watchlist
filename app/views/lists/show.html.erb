<h1><%= @list.name %></h1>
<h3><%= @movies.count %></h3>
<ul>
<% @movies.each do |movie| %>
<% bookmark = Bookmark.find_by(list: @list, movie: movie) %>
<li><%= movie.title %> <%= link_to "Delete", bookmark_path(bookmark), method: :delete %></li>
<% end %>
</ul>
<%= link_to "Add movie", new_list_bookmark_path(@list) %>
<%= link_to "All Lists", lists_path%>
